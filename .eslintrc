{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "globals": {
  },
  "plugins": [
  ],
  "rules": {
    "global-require": "off",
    "import/no-unresolved": "off",
    "no-underscore-dangle": "off",
    "no-new-func": "off",
    "no-param-reassign": "off",
    "no-restricted-syntax": "off",
    "class-methods-use-this": "off",
    "import/no-dynamic-require": "off",
    "no-continue": "off",
    "max-len": "off",
    "no-debugger": "off",
    "func-names": "off",
    "arrow-body-style": "off",
    "no-nested-ternary": "off"
  }
}